[GENERAL]
#verbose = true
polynomialOrder = 3
startFrom = "pipe.fld"+time=0
stopAt = endTime
endTime = 100.0

dt = 5e-3
timeStepper = tombo2
subCyclingSteps = 2

writeControl = simulationTime 
writeInterval = 100

regularization = hpfrt + nModes=1 + scalingCoeff=10
constFlowRate = meanVelocity=1.0 + direction=Z

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = wall
viscosity = -5300
density = 1.0
residualTol = 1e-06

[CASEDATA]
Re_tau = 181
zLength = 6
